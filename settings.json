{
    "database": "audit.db3",
    "get_device_id_uri": "https://us1a.app.anaplan.com/oauth/device/code",
    "get_tokens_uri": "https://us1a.app.anaplan.com/oauth/token",

    "uris": {
        "deviceId": "https://us1a.app.anaplan.com/oauth/device/code",
        "tokens": "https://us1a.app.anaplan.com/oauth/token",
        "users": "https://api.anaplan.com/scim/1/0/v2/Users",
        "auditEvents": "https://audit.anaplan.com/audit/api/1/events?type=all",
        "workspaces": "https://api.anaplan.com/2/0/workspaces?tenantDetails=true",
        "models": "https://api.anaplan.com/2/0/workspaces/{{workspace_id}}/models?modelDetails=true",
        "imports": "https://api.anaplan.com/2/0/workspaces/{{workspace_id}}/models/{{model_id}}/imports/",
        "exports": "https://api.anaplan.com/2/0/workspaces/{{workspace_id}}/models/{{model_id}}/exports/",
        "actions": "https://api.anaplan.com/2/0/workspaces/{{workspace_id}}/models/{{model_id}}/actions/",
        "processes": "https://api.anaplan.com/2/0/workspaces/{{workspace_id}}/models/{{model_id}}/processes/",
        "files": "https://api.anaplan.com/2/0/workspaces/{{workspace_id}}/models/{{model_id}}/files/",
        "cloudWorks": "https://api.cloudworks.anaplan.com/2/0/integrations"
    },

    "targetAnaplanModel": {
        "workspace": "Workspace #1",
        "model": "Audit Report",
        "targetModelObjects": {
            "activityCodesData": {
                "importFile": "ACTIVITY_CODES.csv",
                "importAction": "IMPORT_ACTIVITY_CODES",
                "table": "act_codes",
                "mode": "replace"
            },
            "usersData": {
                "importFile": "USER_LIST.csv",
                "importAction": "IMPORT_USER_LIST",
                "table": "users",
                "mode": "replace"
            },
            "workspacesData": {
                "importFile": "WORKSPACE_LIST.csv",
                "importAction": "IMPORT_WORKSPACE_LIST",
                "table": "workspaces",
                "mode": "replace"
            },
            "modelsData": {
                "importFile": "MODEL_LIST.csv",
                "importAction": "IMPORT_MODEL_LIST",
                "table": "models",
                "mode": "append"
            },
            "actionsData": {
                "importFile": "ACTION_LIST.csv",
                "importAction": "IMPORT_ACTION_LIST",
                "table": "actions",
                "mode": "append"
            },
            "filesData": {
                "importFile": "FILE_LIST.csv",
                "importAction": "IMPORT_FILE_LIST",
                "table": "files",
                "mode": "append"
            },
            "cloudWorksData": {
                "importFile": "CLOUDWORKS_LIST.csv",
                "importAction": "IMPORT_CLOUDWORKS_LIST",
                "table": "cloudworks",
                "mode": "replace"
            },
            "auditData": {
                "importFile": "AUDIT_LOG.csv",
                "importAction": "IMPORT_AUDIT_LOG",
                "table": "events",
                "mode": "replace"
            }
        }
    }
}
